.Contacts {
	grid-width()
	margin: 0 auto
	padding 50px 0
	&__inner {
		display inline-block
		grid-width(6)
		vertical-align top
		
	}
	@media $phone {
		padding 24px 16px
	}
	h1 {
		font-size 32px
		font-weight bold
		margin-bottom 24px
	}
	h2 {
		font-size 24px
		font-weight bold
	}
	img {
		max-width 100%
		margin-top -40px
	}
}
