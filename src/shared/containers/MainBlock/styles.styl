@import '../../styles/common.styl'

.MainBlock {
	& {
		background #EBECEC
		padding 56px 0
		// background-image: url(/bg.png)
		@media $verticalTablet {
			padding 32px 0
		}
		@media $phone {
			padding 0
		}
	}
	&__container {
		container()
		clearfix()
		display flex
	}
	&__content {
		grid-width(9.5)
		float left
		@media $verticalTablet {
			width 100%
		}
	}
}